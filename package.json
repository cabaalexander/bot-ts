{
  "name": "bot-ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "wrangler dev ./src/index.ts",
    "test": "jest --verbose --coverage",
    "test:staged": "yarn jest --bail --findRelatedTests",
    "test:watch": "yarn test --watch",
    "deploy": "wrangler publish ./src/index.ts",
    "lint": "eslint ./src",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "set_secrets": "./set_secrets_from_dev_vars",
    "tunnel": "ngrok http --domain $NGROK_DOMAIN 8787"
  },
  "license": "MIT",
  "dependencies": {
    "discord-interactions": "^3.4.0",
    "dotenv": "^16.3.1",
    "hono": "^3.3.4",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230215.0",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": ">=6.0.0",
    "@typescript-eslint/parser": ">=6.0.0",
    "esbuild": "^0.19.4",
    "esbuild-jest": "^0.5.0",
    "eslint": ">=8.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "husky": "^8.0.3",
    "jest": "^29.1.2",
    "jest-environment-miniflare": "^2.6.0",
    "lint-staged": "^14.0.1",
    "miniflare": "^3.20231002.0",
    "ngrok": "^5.0.0-beta.2",
    "typescript": ">=4.7",
    "wrangler": "^3.10.1"
  }
}
